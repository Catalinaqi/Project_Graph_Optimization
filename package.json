{
  "name": "backend_graphs_optimization",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "keys": "ts-node -r tsconfig-paths/register -r dotenv/config src/common/util/generate-keys.ts",
    "keys:force": "ts-node -r tsconfig-paths/register -r dotenv/config src/common/util/generate-keys.ts --force",
    "dev": "npm run keys && ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/server.ts",
    "build": "tsc && tsc-alias",
    "prestart": "node dist/common/util/generate-keys.js",
    "start": "node dist/server.js",
    "start:dev": "dotenv -e .env -- npm run dev",
    "start:prod": "dotenv -e .env -- npm run build && npm start"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^4.5.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "node-dijkstra": "^2.5.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "express-list-endpoints": "^7.1.1",
    "bcrypt": "^6.0.0",
    "yamljs": "^0.3.0",
    "joi": "^18.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.2.1",
    "@types/node-dijkstra": "^2.5.6",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "cors": "^2.8.5",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "dotenv-cli": "^7.4.2"
  }
}
